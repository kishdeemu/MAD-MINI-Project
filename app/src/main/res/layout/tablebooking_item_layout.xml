<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/blue"
    android:padding="20dp"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:text="Full Name :- "
            android:layout_marginEnd="58dp"
            android:textColor="@color/colorWhite"
            android:fontFamily="@font/roboto_bold"/>

        <TextView
            android:id="@+id/TablefullName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:text="TextView"
            android:textColor="@color/colorWhite"
            android:fontFamily="@font/roboto_bold"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:text="Email :- "
            android:layout_marginEnd="100dp"
            android:textColor="@color/colorWhite"
            android:fontFamily="@font/roboto_bold"/>

        <TextView
            android:id="@+id/tblemail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:text="TextView"
            android:textColor="@color/colorWhite"
            android:fontFamily="@font/roboto_bold"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:text="Phone No. :- "
            android:layout_marginEnd="56dp"
            android:textColor="@color/colorWhite"
            android:fontFamily="@font/roboto_bold"/>

        <TextView
            android:id="@+id/tblphone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:text="TextView"
            android:textColor="@color/colorWhite"
            android:fontFamily="@font/roboto_bold"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:text="No. of People "
            android:layout_marginEnd="48dp"
            android:textColor="@color/colorWhite"
            android:fontFamily="@font/roboto_bold"/>

        <TextView
            android:id="@+id/tblnoOfPeople"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:text="TextView"
            android:textColor="@color/colorWhite"
            android:fontFamily="@font/roboto_bold"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:text="Date :-"
            android:layout_marginEnd="115dp"
            android:textColor="@color/colorWhite"
            android:fontFamily="@font/roboto_bold"/>

        <TextView
            android:id="@+id/tbldate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:text="TextView"
            android:textColor="@color/colorWhite"
            android:fontFamily="@font/roboto_bold"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:text="No. of Hours :- "
            android:layout_marginEnd="40dp"
            android:textColor="@color/colorWhite"
            android:fontFamily="@font/roboto_bold"/>

        <TextView
            android:id="@+id/tblnoOfHours"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:text="TextView"
            android:textColor="@color/colorWhite"
            android:fontFamily="@font/roboto_bold"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:text="Time: "
            android:layout_marginEnd="120dp"
            android:textColor="@color/colorWhite"
            android:fontFamily="@font/roboto_bold"/>

        <TextView
            android:id="@+id/tbltime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:text="TextView"
            android:textColor="@color/colorWhite"
            android:fontFamily="@font/roboto_bold"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:text="@string/comments2"
            android:layout_marginEnd="60dp"
            android:textColor="@color/colorWhite"
            android:fontFamily="@font/roboto_bold"/>

        <TextView
            android:id="@+id/tblcomments"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:text="TextView"
            android:textColor="@color/colorWhite"
            android:fontFamily="@font/roboto_bold"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:text="@string/total_price2"
            android:layout_marginEnd="60dp"
            android:textColor="@color/colorWhite"
            android:fontFamily="@font/roboto_bold"/>

        <TextView
            android:id="@+id/tbltotal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:text="TextView"
            android:textColor="@color/colorWhite"
            android:fontFamily="@font/roboto_bold"/>

    </LinearLayout>




</LinearLayout>